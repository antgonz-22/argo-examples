pipeline {
  agent any
  stages {
    stage('init') {
      steps {
        echo 'Initialize Test'
      }
    }

    stage('Kubectl') {
      steps {
        kubeconfig(serverUrl: 'https://10.95.0.21:6443', caCertificate: '', credentialsId: '')
      }
    }

    stage('get nodes') {
      steps {
        sh 'Kubectl get nodes'
      }
    }

  }
}